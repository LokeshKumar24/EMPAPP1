 <mvc:View controllerName="EA.EmployeeApp2.controller.Home" xmlns:mvc="sap.ui.core.mvc" 
xmlns:f="sap.f" 	xmlns:card="sap.f.cards"
displayBlock="true" xmlns="sap.m" xmlns:tnt="sap.tnt" xmlns:sf="sap.ui.layout.form">
	
		<App >
			<pages>
				<Page  title="Home View" showHeader="false">
					
                   <content>
                  
                   <!-- <Text text="Home"></Text> -->
                   	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="BIRTHDAY'S" subtitle="Wishing a happy and prousporus year a head" />
		</f:header>
		<f:content>
			<List>
				<CustomListItem>
					<HBox alignItems="Center" justifyContent="SpaceBetween">
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Title level="H3" text="{}" />
							<Text text="{}" />
       <Image width="300px"
       src="https://www.readersdigest.ca/wp-content/uploads/2019/02/hilarious-birthday-jokes.jpg"/>
						</VBox>
						<ObjectStatus
							class="sapUiTinyMargin sapUiSmallMarginEnd"
							text="{birth>/bithday/0/name}"
							state="{}" />
					</HBox>
				</CustomListItem>
			</List>
		</f:content>
	</f:Card>
    	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="NEW HIRES" subtitle="Welcome to the Company" />
		</f:header>
		<f:content>
			<List
				showSeparators="None"
				items="{
					path: 'homeModel>/home'
				}">
				<CustomListItem>
					<HBox alignItems="Center" justifyContent="SpaceBetween">
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Title level="H3" text="{}" />
							<Text text="{homeModel>Newhires}" />
						</VBox>
						<ObjectStatus
							class="sapUiTinyMargin sapUiSmallMarginEnd"
							text="{}"
							state="{}" />
					</HBox>
				</CustomListItem>
			</List>
		</f:content>
	</f:Card>
    	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="QUICK LINKS" subtitle="Please refer this links for any quries" />
		</f:header>
		<f:content>
			<List
				showSeparators="None"
				items="{
					path: 'homeModel>/home'
				}">
				<CustomListItem>
					<HBox alignItems="Center" justifyContent="SpaceBetween">
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Title level="H3" text="{}" />
                            <Link href="{homeModel>Quicklinks}" 	target="_blank"
                            text="{homeModel>Quicklinks}"></Link>
							
						</VBox>
						<!-- <ObjectStatus
							class="sapUiTinyMargin sapUiSmallMarginEnd"
							text="{}"
							state="{}" /> -->
					</HBox>
				</CustomListItem>
			</List>
		</f:content>
	</f:Card>
    	<f:Card class="sapUiMediumMargin" width="300px">
		<f:header>
			<card:Header title="NEWS FEED" subtitle="check in the below data for new updates" />
		</f:header>
		<f:content>
			<List
				showSeparators="None"
				items="{
					path: 'homeModel>/home'
				}">
				<CustomListItem>
					<HBox alignItems="Center" justifyContent="SpaceBetween">
						<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
							<Title level="H3" text="{}" />
                          
							<Text text="{homeModel>Newsfeed}" />
						</VBox>
						<ObjectStatus
							class="sapUiTinyMargin sapUiSmallMarginEnd"
							text="{}"
							state="{}" />
					</HBox>
				</CustomListItem>
			</List>
		</f:content>
	</f:Card>
                   </content>
                   
				</Page>
			</pages>
		</App>
	
</mvc:View>
