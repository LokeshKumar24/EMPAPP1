<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
<Dialog  title="Notification"  id="NotifDialog">
   

    	<Table items="{requestModel>/request}" id="idListItem" width="50rem">
			<columns>				
				<Column>
					<Label text="Hi Your  Requests !!!"></Label>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
                        	<!-- <ObjectIdentifier
						title="{requestModel>Reason}"
						text="{requestModel>Assets}"/> -->
                        
			<NotificationListGroup
				title="{requestModel>Reason}"
				showCloseButton="true"
				collapsed="true"
				close=".onItemClose">
				
				<NotificationListItem
					title="{requestModel>Reason}"
					description="{requestModel>Reason} "
					showCloseButton="true"
					datetime="1 hour"
					unread="true"
					priority="None"
					close=".onItemClose"
					press=".onListItemPress"
					/>
				

			
			</NotificationListGroup>
                       
						
					</cells>
				</ColumnListItem>
			</items>
		</Table>
    <!-- <Label text=""></Label>
    <Label text="Reason:" ></Label>
    <Text text=""></Text>
    -->
  

            <beginButton>
			
            <Button press="onClosePopover" text="Close" ></Button>
		</beginButton>
            
              </Dialog> 
 
  
	
</core:FragmentDefinition>