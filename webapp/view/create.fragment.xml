<core:FragmentDefinition xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" 
xmlns:sf="sap.ui.layout.form" 	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
	xmlns:smartTable="sap.ui.comp.smarttable">
    <Dialog  title="Request" id="DialogCreateProject" >

	
		
         <VBox class="sapUiSmallMargin">
		<sf:SimpleForm id="SimpleFormChange353"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Create New Project "
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<sf:content>
				<Label text=""/> 
				<Input id="Pid" value=""  placeholder="Enter Searial Number"  />
                <Label text=""/> 
				<Input id="eid" value=""  placeholder="Enter Emp ID"  />
                <Label text=""/> 
				<Input id="name" value=""  placeholder="Enter Your Name"  />
                <Label text="" />
				<Input id="Pname" value=""  placeholder="Enter your Projectname"/>
                <Label text=""/>
				<Input id="pdetails" value="" placeholder="Projectdetails"/>
                <Label text=""/>
                
		<DatePicker
			id="PSDP"
			placeholder="Enter StartDate"
			change="handleChange"
			class="sapUiSmallMarginBottom" />
				
                <Label text=""/>
                <DatePicker
			id="PEDP"
			placeholder="Enter EndDate"
			change="handleChange"
			class="sapUiSmallMarginBottom"/>
				
				
				<Label text=""/>
				<Input id="pstatus" value="" placeholder="Status"/>
				
                <Label></Label>
				<Button text="Save" type="Emphasized" press="onCreate"></Button>
                 </sf:content>
		</sf:SimpleForm>
	</VBox>
            
			
                 
	<beginButton>
    <Button text="close" icon="sap-icon://decline" press="onClose" type="Reject"></Button>
    </beginButton>			
			
	</Dialog>
</core:FragmentDefinition>